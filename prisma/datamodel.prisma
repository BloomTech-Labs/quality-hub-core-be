type User {
  id: ID! @id
  first_name: String!
  last_name: String!
  email: String! @unique
  password: String!
  city: String!
  state: String!
  gender: String
  bio: String
  image_url: String
  portfolio_url: String
  linkedin_url: String
  github_url: String
  personal_url: String
  blog_url: String
  twitter_url: String
  activated_stripe: Boolean
}

