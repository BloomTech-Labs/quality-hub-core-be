type User {
  id: ID! @id
  first_name: String!
  last_name: String!
  email: String! @unique
  password: String!
  city: String!
  state: String!
  gender: String
  bio: String
  image_url: String
  portfolio_url: String
  linkedin_url: String
  github_url: String
  personal_url: String
  blog_url: String
  twitter_url: String
  activated_stripe: Boolean
  # coach_bookings: [Booking] @relation(name: "CoachBookings", onDelete: CASCADE)
  # seeker_bookings: [Booking] @relation(name: "SeekerBookings", onDelete: CASCADE)
  availability: [Availability]
}

type Availability {
  id: ID! @id
  dayOfWeek: String!
  start_hour: Int!
  start_minute: Int!
  end_hour: Int!
  end_minute: Int!
  user: User!
}

